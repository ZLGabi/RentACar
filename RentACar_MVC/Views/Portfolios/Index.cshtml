@model IEnumerable<RentACar.ServicesInfrastructure.DTO.PortfolioListDTO>

<div class="row">
    @foreach (var portfolios in Model)
    {
        <div  class="col-md-3">
            <a href="@Url.Action("Details", new { id = portfolios.PortfolioId })">
            <div class="bg-secondary text-light mt-md-4 p-2">
                <img src="@portfolios.Photo.Url" alt="Portfolio Photo"/>
                <h5>@portfolios.Name</h5>
            </div>
            </a>
        </div>
    }
</div>
